<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bh.mall.dao.IWareHouseLogDAO">
	<resultMap id="wareHouseLog_DB_Result" type="wareHouseLog">
		<result column="code" property="code" />
		<result column="ref_no" property="refNo" />
		<result column="ware_house_code" property="wareHouseCode" />
		<result column="type" property="type" />
		<result column="product_code" property="productCode" />
		<result column="product_name" property="productName" />
		<result column="product_specs_code" property="productSpecsCode" />
		<result column="product_specs_name" property="productSpecsName" />
		<result column="tran_number" property="tranNumber" />
		<result column="before_number" property="beforeNumber" />
		<result column="after_number" property="afterNumber" />
		<result column="price" property="price" />
		<result column="amount" property="amount" />
		<result column="change_product_code" property="changeProductCode" />
		<result column="change_product_name" property="changeProductName" />
		<result column="change_specs_code" property="changeSpecsCode" />
		<result column="change_specs_name" property="changeSpecsName" />
		<result column="change_number" property="changeNumber" />
		<result column="change_price" property="changePrice" />
		<result column="change_amount" property="changeAmount" />
		<result column="status" property="status" />
		<result column="approver" property="approver" />
		<result column="approve_datetime" property="approveDatetime" />
		<result column="approve_note" property="approveNote" />
		<result column="biz_type" property="bizType" />	
		
		<result column="biz_note" property="bizNote" />
	</resultMap>
	<sql id="where_condition">
		<trim prefix="WHERE" prefixOverrides="AND | OR">
			<if test="code != null and code != '' ">
				AND t.code = #{code}
			</if>
		</trim>
	</sql>
	<select id="select_wareHouseLog" parameterType="wareHouseLog" resultMap="wareHouseLog_DB_Result">
		SELECT
		t.code
		,t.ref_no
		,t.ware_house_code
		,t.type
		,t.product_code
		,t.product_name
		,t.product_specs_code
		,t.product_specs_name
		,t.tran_number
		,t.before_number
		,t.after_number
		,t.price
		,t.amount
		,t.change_product_code
		,t.change_product_name
		,t.change_specs_code
		,t.change_specs_name
		,t.change_number
		,t.change_price
		,t.change_amount
		,t.status
		,t.apply_user
		,t.apply_datetime
		,t.apply_note
		,t.approver
		,t.approve_datetime
		,t.approve_note
		,t.biz_type
		
		,t.biz_note
		FROM
		tbh_ware_house_log t
		<include refid="where_condition" />
		<trim prefix="ORDER BY ">
			<if test="order != null and order != '' ">
				t.${order}
			</if>
		</trim>
	</select>

	<select id="select_wareHouseLog_count" parameterType="wareHouseLog"
		resultType="java.lang.Long">
		SELECT count(1) FROM tbh_ware_house_log t
		<include refid="where_condition" />
	</select>

	<insert id="insert_wareHouseLog" parameterType="wareHouseLog">
		INSERT INTO
		tbh_ware_house_log(
		code
		,ref_no
		,ware_house_code
		,type
		,product_code
		,product_name
		,product_specs_code
		,product_specs_name
		,tran_number
		,before_number
		,after_number
		,price
		,amount
		,change_product_code
		,change_product_name
		,change_specs_code
		,change_specs_name
		,change_number
		,change_price
		,change_amount
		,status
		,apply_user
		,apply_datetime
		,apply_note
		,approver
		,approve_datetime
		,approve_note
		,biz_type
		
		,biz_note
		)
		VALUES(
		#{code}
		,#{refNo}
		,#{wareHouseCode}
		,#{type}
		,#{productCode}
		,#{productName}
		,#{productSpecsCode}
		,#{productSpecsName}
		,#{tranNumber}
		,#{beforeNumber}
		,#{afterNumber}
		,#{price}
		,#{amount}
		,#{changeProductCode}
		,#{changeProductName}
		,#{changeSpecsCode}
		,#{changeSpecsName}
		,#{changeNumber}
		,#{changePrice}
		,#{changeAmount}
		,#{status}
		,#{applyUser}
		,#{applyDatetime}
		,#{applyNote}
		,#{approver}
		,#{approveDatetime}
		,#{approveNote}
		,#{bizType}
		,#{bizNote}
		)
	</insert>

	<update id="update_wareHouseLog" parameterType="wareHouseLog">
		UPDATE tbh_ware_house_log
		SET
		ref_no = #{refNo}
		,ware_house_code = #{wareHouseCode}
		,type = #{type}
		,product_code = #{productCode}
		,product_name = #{productName}
		,product_specs_code = #{productSpecsCode}
		,product_specs_name = #{productSpecsName}
		,price = #{price}
		,amount = #{amount}
		,change_product_code = #{changeProductCode}
		,change_product_name = #{changeProductName}
		,change_specs_code = #{changeSpecsCode}
		,change_specs_name = #{changeSpecsName}
		,change_number = #{changeNumber}
		,change_price = #{changePrice}
		,change_amount = #{changeAmount}
		,status = #{status}
		,apply_user = #{applyUser}
		,apply_datetime = #{applyDatetime}
		,apply_note = #{applyNote}
		,approver = #{approver}
		,approve_datetime = #{approveDatetime}
		,approve_note = #{approveNote}
		WHERE code = #{code}
	</update>


</mapper>